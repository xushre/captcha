{
    "name": "xushre/laravel-captcha",
    "type": "package",
    "description": "Laravel 5 Captcha Package",
    "keywords": [
        "laravel5 Captcha",
        "laravel5 Security",
        "Captcha"
    ],
    "homepage": "https://github.com/xushre/laravel-captcha",
    "license": "MIT",
    "authors": [
        {
            "name": "xushre",
            "email": "xushre@qq.com",
            "homepage": "https://github.com/xushre",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "ext-gd": "*",
        "illuminate/config": "5.*",
        "illuminate/filesystem": "5.*",
        "illuminate/support": "5.*",
        "illuminate/hashing": "5.*",
        "illuminate/routing": "5.*",
        "intervention/image": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Xushre\\Captcha\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests"
        ],
        "psr-4": {
            "Xushre\\Test\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Xushre\\Captcha\\CaptchaServiceProvider"
            ],
            "aliases": {
                "Captcha": "Xushre\\Captcha\\Facades\\Captcha"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
